{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "auto",
        "source": "/usr/share/pixmaps/magicos-logo.png",
        "width": 30,
        "height": 15,
        "padding": {
            "top": 1,
            "left": 2
        }
    },
    "display": {
        "separator": " → ",
        "color": {
            "keys": "magenta",
            "title": "blue"
        }
    },
    "modules": [
        {
            "type": "title",
            "format": "{user-name-colored}@{host-name-colored}"
        },
        {
            "type": "separator",
            "string": "─"
        },
        {
            "type": "os",
            "key": "OS",
            "keyColor": "magenta"
        },
        {
            "type": "kernel",
            "key": "Kernel",
            "keyColor": "magenta"
        },
        {
            "type": "packages",
            "key": "Packages",
            "keyColor": "magenta"
        },
        {
            "type": "shell",
            "key": "Shell",
            "keyColor": "magenta"
        },
        {
            "type": "de",
            "key": "DE",
            "keyColor": "magenta"
        },
        {
            "type": "wm",
            "key": "WM",
            "keyColor": "magenta"
        },
        {
            "type": "wmtheme",
            "key": "Theme",
            "keyColor": "magenta"
        },
        {
            "type": "terminal",
            "key": "Terminal",
            "keyColor": "magenta"
        },
        {
            "type": "cpu",
            "key": "CPU",
            "keyColor": "blue"
        },
        {
            "type": "gpu",
            "key": "GPU",
            "keyColor": "blue"
        },
        {
            "type": "memory",
            "key": "Memory",
            "keyColor": "blue"
        },
        {
            "type": "disk",
            "key": "Disk",
            "keyColor": "blue"
        },
        {
            "type": "uptime",
            "key": "Uptime",
            "keyColor": "cyan"
        },
        {
            "type": "colors",
            "paddingLeft": 2,
            "symbol": "circle"
        }
    ]
}